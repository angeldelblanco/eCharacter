<?xml version="1.0" encoding="UTF-8"?>
<family xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../family.xsd">
	<metadata>
		<name>idNameFamily</name>
		<description>idDescriptionFamily</description>
		<author>idAuthor</author>
		<languagesPath>assets\Locale\Medieval</languagesPath>
		<iconPath></iconPath>
	</metadata>
	<stages>
	<!-- Revisar que controladores poner -->
		<scaleStage stageLabel="idMultiStageLabel1" idPanel="ScalePanel">
			<boneController controllerLabel="idBoneControler1" idController="head"/>
			<boneController controllerLabel="idBoneControler2" idController="arms"/>
			<boneController controllerLabel="idBoneControler3" idController="hands"/>
			<boneController controllerLabel="idBoneControler4" idController="legs"/>
			<boneController controllerLabel="idBoneControler5" idController="feet"/>
		</scaleStage>
		<multiStage stageLabel="idMultiStageLabel2">
			<subStage subStageType="textureSubStage" subStageLabel="idTextureSubStageLabel1" idPanel="skinsPanel" multiselection="false"/>
		</multiStage>
		<multiStage stageLabel="idMultiStageLabel3">
			<subStage subStageType="textureSubStage" subStageLabel="idTextureSubStageLabel2" idPanel="hairsPanel" multiselection="false"/>
		</multiStage>
		<multiStage stageLabel="idMultiStageLabel4">
			<subStage subStageType="textureSubStage" subStageLabel="idTextureSubStageLabel3" idPanel="eyesPanel" multiselection="false"/>
		</multiStage>
		<multiStage stageLabel="idMultiStageLabel5">
			<subStage subStageType="textureSubStage" subStageLabel="idTextureSubStageLabel4" idPanel="shirtsPanel" multiselection="true"/>
			<subStage subStageType="textureSubStage" subStageLabel="idTextureSubStageLabel5" idPanel="trousersPanel" multiselection="true"/>
		</multiStage>
		<multiStage stageLabel="idMultiStageLabel6">
			<subStage subStageType="textureSubStage" subStageLabel="idTextureSubStageLabel6" idPanel="shoesPanel" multiselection="false"/>
		</multiStage>
		<multiStage stageLabel="idMultiStageLabel7">
			<subStage subStageType="textureSubStage" subStageLabel="idTextureSubStageLabel7" idPanel="accesoriesPanel" multiselection="true"/>
		</multiStage>
		<!-- Revisar la etapa de animaciÃ³n -->
		<animationStage stageLabel="idAnimationStageLabel1">
			<!-- Dar valores. Los que hay son aleatorios -->
			<fps qualityLabel="idQualityLabel1" fpsValue="3"/>
			<fps qualityLabel="idQualityLabel2" fpsValue="2"/>
			<fps qualityLabel="idQualityLabel3" fpsValue="1"/>
			<camera cameraLabel="idCameraLabel1">
				<upAxis valueX="0.0" valueY="1.0" valueZ="0.0"/>
				<location valueX="0.0" valueY="0.0" valueZ="10.0"/>
				<direction valueX="0.0" valueY="0.0" valueZ="-1.0"/>
			</camera>
			<camera cameraLabel="idCameraLabel2">
				<upAxis valueX="0.0" valueY="1.0" valueZ="0.0"/>
				<location valueX="-3.0" valueY="5.0" valueZ="10.0"/>
				<direction valueX="0.0" valueY="0.0" valueZ="-1.0"/>
			</camera>
		</animationStage>
	</stages>
	<modelsRef>
		<modelRef>
			<modelLabel>idModelLabel1</modelLabel>	
			<iconPath>assets\Interface\Icons\Medieval\IconMale.png</iconPath>
			<modelPath>assets\XML Configuration\models\Medieval\Male.xml</modelPath>
		</modelRef>
		<modelRef>
			<modelLabel>idModelLabel2</modelLabel>
			<iconPath>assets\Interface\Icons\Medieval\IconFemale.png</iconPath>
			<modelPath>assets\XML Configuration\models\Medieval\Female.xml</modelPath>
		</modelRef>	
	</modelsRef>
</family>
