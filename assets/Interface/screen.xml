								<!-- Space -->
							</panel>
						</panel>
					</panel>
				</panel>
			</panel>
			<!-- Change screen Panel -->
			<panel id="panel_change_screen" height="15%" width="30%" childLayout="horizontal" align="right">
				<panel id="panel_screenleft" height="66%" width="33%" childLayout="center" valign="center" backgroundImage="${CALL.getButton(previous)}">
					<text id="previousText" text="" font="Interface/Fonts/Default.fnt" width="100%" height="100%" />
