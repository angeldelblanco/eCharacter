				</panel>
				<panel id="panel_screenright" height="66%" width="33%" childLayout="center" valign="center" backgroundImage="${CALL.getButton(next)}">
					<text id="finishText" text="" font="Interface/Fonts/Default.fnt" width="100%" height="100%" />
					<interact onClick="export()"/>
				</panel>
			</panel>
        </layer>
